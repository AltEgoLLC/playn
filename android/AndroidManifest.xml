<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="playn.android"
      android:versionCode="1"
      android:versionName="1.0">
          
    <!--
    <permission android:name="playn.android.permission.C2D_MESSAGE" android:protectionLevel="signature" />
    <uses-permission android:name="playn.android.permission.C2D_MESSAGE" /> 
    -->
          
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        
        <!-- Billing -->
        <service android:name=".BillingService" />

        <receiver android:name=".BillingReceiver">
          <intent-filter>
            <action android:name="com.android.vending.billing.IN_APP_NOTIFY" />
            <action android:name="com.android.vending.billing.RESPONSE_CODE" />
            <action android:name="com.android.vending.billing.PURCHASE_STATE_CHANGED" />
          </intent-filter>
        </receiver>     
        <!-- End Billing -->
        
        <!-- GCM STUFF -->
        <receiver android:name="com.google.android.gcm.GCMBroadcastReceiver" android:permission="com.google.android.c2dm.permission.SEND" >
          <intent-filter>
            <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            <action android:name="com.google.android.c2dm.intent.REGISTRATION" />
            <category android:name="com.altego.riot.android" />
          </intent-filter>
        </receiver>
        
        <service android:name="playn.android.gcm.GCMIntentService" />
        <!-- END GCM STUFF -->
        
    </application>

    
</manifest>
